#include "main.h"

/**
 *_strcat - function to concatenate two strings
 *@dest: the string to concatenate to
 *@src: the string to add
 *Return: dest
 */

char *_strcat(char *dest, char *src)
{
	int x, y;

	for (x = 0; dest[x]; x++)
		;
	for (y = 0; src[y]; y++)
	{
		dest[x] = src[y];
		y++;
	}
	dest[x] = '\0';
	return (dest);
}
